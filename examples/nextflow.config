// Configuration file for nf-pregex examples

// Enable the nf-pregex plugin
plugins {
    id 'nf-pregex@0.1.0'
}

// Example workflow settings
params {
    outdir = 'results'
}

// Process settings
process {
    errorStrategy = 'terminate'
}

// Execution report
report {
    enabled = true
    file = "${params.outdir}/report.html"
}
